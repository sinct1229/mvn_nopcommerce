<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="nopcommerce">
	<parameter name="appURL"
		value="https://demo.nopcommerce.com/"></parameter>
	<parameter name="browser" value="chrome"></parameter>
	<test name="run on Chrome">
		<classes>
		<class name = "com.nopcommerce.authentication.Register"></class>
		<class name = "com.nopcommerce.authentication.Login"></class>
		<class name = "com.nopcommerce.myAccount.CustomerInfo"></class>
		<class name = "com.nopcommerce.myAccount.Addresses"></class>
		<class name = "com.nopcommerce.myAccount.ChangePassword"></class>
		<class name = "com.nopcommerce.search.Search"></class>
		<class name = "com.nopcommerce.sort.Sort"></class>
		<class name = "com.nopcommerce.sort.Paging"></class>
		</classes>
	</test>
</suite> 